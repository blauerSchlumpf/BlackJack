<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Width="300" Height="150" WindowStartupLocation="CenterScreen"
		x:Class="BlackJack.Views.MessageBoxWindow">
	<StackPanel>
		<TextBlock Text="{Binding Message}" Margin="10" HorizontalAlignment="Center" TextWrapping="Wrap" />
		<Button Content="OK" Command="{Binding CloseCommand}" HorizontalAlignment="Center" Margin="10" />
	</StackPanel>
</Window>
